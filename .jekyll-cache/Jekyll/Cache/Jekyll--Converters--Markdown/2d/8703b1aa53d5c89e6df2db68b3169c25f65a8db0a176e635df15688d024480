<div id="aScherer:2012ke" style="display:none"><pre>@article{Scherer:2012ke,
  author = {Scherer, Sebastian and Chamberlain, Lyle and Singh, Sanjiv},
  doi = {10.1016/j.robot.2012.09.004},
